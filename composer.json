{
    "name": "rubensrocha/lara-install-wizard",
    "description": "Laravel application installer with version and auth package selector.",
    "keywords": ["laravel","jetstream","ui","laravel auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Rubens Rocha",
            "email": "smartyscripts@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "symfony/console": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/larawizard"
    ],
    "autoload": {
        "psr-4": {
            "Rubensrocha\\LaraWizard\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rubensrocha\\LaraWizard\\Console\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
